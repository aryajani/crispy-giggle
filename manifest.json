{
"name": "Extension with GAPI tutorial",
"version": "1.0",
"description": "Uses OAuth to connect to Google's Spreadsheet API",
"manifest_version": 2,
"browser_action": {
    "default_title": "GAPI Tutorial"
 },
"background": {
    "page": "background.html",
    "persistent": false
},
"key":"",
"oauth2": {
    "client_id": "155835288526-k9pubhot3ebqf3hc8unpk8buq4e1sbvf.apps.googleusercontent.com",
    "scopes":["https://www.googleapis.com/auth/spreadsheets"]
      },
    "permissions": ["identity"],
    "content_security_policy": "script-src 'self' https://apis.google.com/; object-src 'self'",
    "content_scripts": [
        {
          "matches": [
            "https://bumbu.me/*"
          ],
          "js": [
            "inject.js"
          ],
          "run_at": "document_end"
        }
      ]
}
